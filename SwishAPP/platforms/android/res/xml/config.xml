<?xml version='1.0' encoding='utf-8'?>
<widget id="ca.edumedia.dure0018.SwishAPP" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <name>SwisshAPP</name>
    <description>
       MAD9014 Final 2016 By Jonathan Dure
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Jonathan Dure
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <splash density="land-hdpi" src="platforms/android/res/drawable-land-hdpi/screen.png" />
    <splash density="land-ldpi" src="platforms/android/res/drawable-land-ldpi/screen.png" />
    <splash density="land-mdpi" src="platforms/android/res/drawable-land-mdpi/screen.png" />
    <splash density="land-xhdpi" src="platforms/android/res/drawable-land-xhdpi/screen.png" />
    <splash density="port-hdpi" src="platforms/android/res/drawable-port-hdpi/screen.png" />
    <splash density="port-ldpi" src="platforms/android/res/drawable-port-ldpi/screen.png" />
    <splash density="port-mdpi" src="platforms/android/res/drawable-port-mdpi/screen.png" />
    <splash density="port-xhdpi" src="platforms/android/res/drawable-port-xhdpi/screen.png" />
    <icon density="ldpi" src="platforms/android/res/mipmap-ldpi/icon.png" />
    <icon density="mdpi" src="platforms/android/res/mipmap-mdpi/icon.png" />
    <icon density="hdpi" src="platforms/android/res/mipmap-hdpi/icon.png" />
    <icon density="xhdpi" src="platforms/android/res/mipmap-xhdpi/icon.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="5000" />
</widget>
